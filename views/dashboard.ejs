<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= name %></p>
<form action="/todo/addTodo" method="POST">
    <div class="form-group">
        <input type="text" id="todoName" name="todoName" class="form-control" placeholder="Insert a todo ..." />
    </div>
    <button type="submit" class="btn btn-secondary btn-block">Add todo</button>
    <br>
</form>
<a href="/todo/viewTodo" class="btn btn-info">View Todos</a>
<br>
<br>
<a href="/users/logout" class="btn btn-primary ">Logout</a>
